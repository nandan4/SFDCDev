    <validationRules>
        <fullName>Release_Actual_End_Date_Validation</fullName>
        <active>true</active>
        <description>Actual end date should not be greater  than today&apos;s date</description>
        <errorConditionFormula>Release_Actual_End_Date__c  &gt; Today()</errorConditionFormula>
        <errorDisplayField>Release_Actual_End_Date__c</errorDisplayField>
        <errorMessage>Actual end date should not be greater  than today&apos;s date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Releases_Date_Validation</fullName>
        <active>true</active>
        <description>Releases_Date_Validation</description>
        <errorConditionFormula>OR( Release_Actual_End_Date__c  &lt;  Release_Start_Date__c ,  Release_Planned_End_Date__c  &lt;  Release_Start_Date__c )</errorConditionFormula>
        <errorMessage>Release Start/End Date should be later than Release Start Date.</errorMessage>
    </validationRules>
