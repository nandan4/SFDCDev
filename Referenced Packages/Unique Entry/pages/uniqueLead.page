                var goto = '{!goToURL}';
                if(goto != ''){           
                    if(goto == 'tab'){
                        alert('Assignment Rules have reassigned the record you just created and as a result you don\'t have access to view it, but rest assured it was created successfully!');
                        window.location='{!URLFOR($Action.Lead.Tab, $ObjectType.Lead)}';
                    }
                    else if(goto == 'record'){
                        var recId = '{!lead.id}';
                        window.location='{!$Site.prefix}/' + recId.replace(/[^a-zA-Z0-9]/g,'');  //strip out anything but letters and numbers just in case
                    }
                    else if(goto == 'new'){
                        window.location='{!URLFOR($Action.Lead.New, $ObjectType.Lead)}';
                    }
                    else if(goto == 'broadlook'){
                        var recId = '{!lead.id}';
                        window.location='{!$Site.prefix}/apex/blds__ShieldUpdate?newid=' + recId.replace(/[^a-zA-Z0-9]/g,'');  //strip out anything but letters and numbers just in case
                    }
                }
