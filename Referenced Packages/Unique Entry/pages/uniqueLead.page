            seekStatusOff();
        }
        
        function addToOnload(fn){
            var old = window.onload;
            window.onload = function(){
                old(); fn();
            }
        }
        var seekStatus;
        function seekStatusOn(){
            $("#seekStatus").show();
            seekStatus = true;
        }
        
        function seekStatusOff(){
            $("#seekStatus").hide();
            seekStatus = false;
        }
        
